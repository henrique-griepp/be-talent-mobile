{
  "expo": {
    "name": "be-talent",
    "slug": "be-talent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon-be.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/logo-be.png",
      "resizeMode": "contain",
      "backgroundColor": "#0500ff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo-be.png",
        "backgroundColor": "#0500ff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-font",
        {
          "fonts": ["./assets/fonts/times.otf"]
        }
      ]
    ]
  }
}
